{
  "name": "@norchain/api",
  "version": "2.0.0",
  "description": "Production-ready NorChain API - Unified backend for Explorer, Wallet, Exchange, and all ecosystem services",
  "author": "Nor Chain Team",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "test:all": "npm run test && npm run test:e2e && npm run test:integration",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/migrations/runner.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/migrations/runner.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/migrations/runner.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "db:setup": "./scripts/setup-supabase.sh",
    "db:setup:simple": "./scripts/setup-supabase-simple.sh",
    "db:setup:sql": "ts-node -r tsconfig-paths/register scripts/setup-supabase-via-client.ts",
    "db:verify": "ts-node -r tsconfig-paths/register scripts/verify-supabase-setup.ts",
    "db:realtime": "ts-node -r tsconfig-paths/register scripts/enable-supabase-realtime.ts",
    "db:complete": "./scripts/setup-supabase-complete.sh",
    "db:test": "ts-node -r tsconfig-paths/register scripts/test-supabase-direct.ts",
    "db:check-realtime": "ts-node -r tsconfig-paths/register scripts/check-realtime-status.ts",
    "db:setup-rls": "ts-node -r tsconfig-paths/register scripts/setup-rls-policies.ts",
    "db:enable-realtime": "ts-node -r tsconfig-paths/register scripts/enable-realtime.ts",
    "db:apply-all": "ts-node -r tsconfig-paths/register scripts/apply-rls-realtime.ts",
    "db:cleanup": "./scripts/cleanup-supabase.sh",
    "db:reset": "./scripts/reset-supabase.sh",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:generate": "node scripts/generate-docs.js"
  },
  "dependencies": {
    "@nestjs/apollo": "13.2.1",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/event-emitter": "3.0.1",
    "@nestjs/graphql": "13.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/throttler": "^5.0.1",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.3.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/multer": "2.0.0",
    "@types/sharp": "0.31.1",
    "apollo-server-express": "3.13.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.3.2",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "ethers": "^6.9.2",
    "graphql": "16.12.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "nest-winston": "^1.9.4",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "0.34.5",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.17",
    "uuid": "13.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.35",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@config/(.*)$": "<rootDir>/config/$1",
      "^@modules/(.*)$": "<rootDir>/modules/$1"
    }
  }
}
