    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "4.7.0",
    "@vitest/coverage-v8": "1.6.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.0",
    "jsdom": "23.2.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vitest": "1.6.1"
  }
